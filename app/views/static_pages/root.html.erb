<div id="main"></div>
<div class="container-fluid tree">
  <div class="row">
    <div class="col-md-2 col-sm-4 col-lg-2">
      <form>
        <div class="form-group">
          <input
            class="form-control"
            name="wish[from_email]"
            type="email"
            placeholder="From:"
            value="<%= @current_email %>"></input>
        </div>
        <div class="form-group">
          <input
            class="form-control"
            name="wish[to_email]"
            placeholder="To:"
            type="email"></input>
        </div>
        <div class="form-group">
          <textarea class="form-control" name="wish[note]">Happy Holidays!</textarea>
        </div>
        <input class="btn btn-primary" type="submit" value="Send Cheer!">
      </form>
    </div>
  </div>
</div>

<script>
  $(function () {
    App.initialize();
  });
</script>
